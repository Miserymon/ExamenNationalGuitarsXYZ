<!-- res/values/themes.xml -->
<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.ExamenNationalDiego" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/azulclarito</item>
        <item name="colorPrimaryVariant">@color/azulclarito</item>
        <item name="colorOnPrimary">@color/azulclarito</item>
        <!-- secondary-->
        <item name="colorSecondary">@color/azulmar</item>
        <item name="colorSecondaryVariant">@color/azulmar</item>
        <item name="colorOnSecondary">@color/azulmar</item>
        <!-- bar color -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>

    <style name="roundedImageViewRouded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="Theme.ExamenNationalDiego" parent="Base.Theme.ExamenNationalDiego" />
</resources>
